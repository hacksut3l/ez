.pre_btn2 { margin-left: 12px; float: left; border-radius: 3px; border: 0; color: #FFF; text-align: center; font-family: 'Conv_Helvetica-Light'; padding: 3px 11px; font-size: 12px; background: -webkit-gradient(linear, 0% 0%, 0% 99%, from(#005e9e), to(#006dc4)); /* Safari 5.1, Chrome 10+ */
