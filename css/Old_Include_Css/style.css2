.pre_btns a:hover { background: #6bcfd7; /* Old browsers */ background: -moz-linear-gradient(top, #6bcfd7 1%, #3dc1cb 100%); /* FF3.6+ */ background: -webkit-gradient(linear, left top, left bottom, color-stop(1%, #6bcfd7), color-stop(100%, #3dc1cb)); /* Chrome,Safari4+ */ background: -webkit-linear-gradient(top, #6bcfd7 1%, #3dc1cb 100%); /* Chrome10+,Safari5.1+ */ background: -o-linear-gradient(top, #6bcfd7 1%, #3dc1cb 100%); /* Opera 11.10+ */ background: -ms-linear-gradient(top, #6bcfd7 1%, #3dc1cb 100%); /* IE10+ */ background: linear-gradient(to bottom, #6bcfd7 1%, #3dc1cb 100%); /* W3C */  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#6bcfd7', endColorstr='#3dc1cb', GradientType=0 ); /* IE6-9 */
